
/* 用户头像  */
.W_my_img {width: 60px;height: 60px;border-radius: 50%;}
.W_top_img {position: absolute;left: 10rpx;top:50rpx;width: 50px;height: 50px;z-index: -1; 
  animation-name: first;
  animation-duration: 60s;
   animation-timing-function: linear; 
  animation-delay: 0s;
   animation-iteration-count: infinite; 
   animation-direction: alternate; 
  /* animation-play-state: running; */
  }

/* 中间卡片  */
.W_my_m {width: 670rpx;height: 92rpx;line-height: 92rpx;box-shadow: -2rpx 4rpx 8rpx #EDEEF0;margin: 40rpx auto;border-radius: 8rpx;}
.W_my_m view {width: 50%;}

/* 列表  */
.W_my_list1 {width: 100%;height: 90rpx;line-height: 90rpx;padding: 0 35rpx;border-bottom: 1px #FAFAFA solid;box-sizing: border-box;}
.W_my_list1 image {width: 24px;height: 24px;margin-top: 20rpx;}
.W_my_list1 text {font-size: 17px;color: #333;}
.no_border {border: none;}


/* 视频  */
.W_mask {width: 100%;height: 100%;position: fixed;left: 0;top: 0;z-index: 99;background: rgba(0, 0, 0, 0.6);}
.W_videobox {width: 70%;height: auto;position: fixed;top: 20%;z-index: 99999;background: #fff;left: 15%;border-radius: 15rpx;overflow: hidden;}
.W_pop_btn {border-top: 1px solid #EDEEF0;margin-top: 50rpx;}
.W_pop_btn view:nth-child(1):after {content: "";width: 2rpx;height: 70rpx;background: #EDEEF0;position: absolute;z-index: 9999;left: 50%;}
.W_pop_btn view{width: 50%;height: 70rpx;line-height: 70rpx;}

@keyframes first {
  0%{left:0;top:10%;transform:rotate(0deg);}
  50%{left: 80%;top: 0;transform: rotate(-10deg);display: none;}
}
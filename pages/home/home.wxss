

.W_home_sw {width: 100%;height: 400rpx;}
.W_home_sw image{width: 100%;height: 400rpx;}

/* 首页导航  */
.W_nav {}
.W_nav_list1 {width: 25%;}
.W_nav_list1 image {width: 60px;height: 60px;}

/* 店长推荐  */
.W_boss {width: 95%;margin: 0 auto;}
.W_boss_list {width: 33.3%;text-align: center;}
.W_boss_list1 image{width: 200rpx;height: 200rpx;border-radius: 8rpx;}
/* .W_boss_list1 {width: 200rpx;} */
.W_boss_list1 image{width: 200rpx;height: 200rpx;}
.W_boss_name,.W_boss_money {margin-top: 15rpx;text-align: left;margin-left: 8%;width: 200rpx;}

/* 我们的技师  */
.W_our {}
.W_our_list1 {width: 25%;}
.W_our_list1 image {width: 70px;height: 70px;border-radius: 50%}
.W_our_list1_name {margin-top: 10rpx;padding: 0 10rpx;box-sizing: border-box;}

/* 我们的团队  */
.W_team {width: 90%;margin: 0 auto;}
.W_team_sw_box {padding: 20rpx;box-sizing: border-box;background: #FAFAFA;box-shadow: -2rpx 2rpx 10rpx #ccc;margin-bottom: 20rpx;}
.W_team_sw {width: 630rpx;height:374rpx;}
.W_team_sw image{width: 630rpx;height:374rpx;}


/* 遮罩层  */
.W_mask {width: 100%;height: 100%;position: fixed;left: 0;top: 0;z-index: 99;background: rgba(0, 0, 0, 0.6);}
.W_pop {border-radius: 15rpx;overflow: hidden;background: #fff;width: 630rpx;position: fixed;margin-left: -315rpx;left: 50%;z-index: 999;top: 5%;}
.W_pop_img {}
.W_pop_img image {width: 630rpx;height: 630rpx;}
.W_nowbtn {width: 160rpx;height: 56rpx;text-align: center;line-height: 56rpx;border-radius: 28rpx;}
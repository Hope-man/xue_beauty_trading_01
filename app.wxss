/* 左右渐变线条  */
.line::after {
  content: "";
  width: 20%;
  height: 2rpx;
  position: absolute;
  z-index: 9;
  margin-top: 20rpx;
  left: 20%;
  background: -webkit-linear-gradient(left, #ccc , #fff); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #ccc, #fff); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #ccc, #fff); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #ccc , #fff); /* 标准的语法 */
}
.line::before{
  content: "";
  width: 20%;
  height: 2rpx;
  position: absolute;
  z-index: 9;
  margin-top: 18rpx;
  right: 20%;
  background: -webkit-linear-gradient(left, #fff , #ccc); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #fff, #ccc); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #fff, #ccc); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #fff , #ccc); /* 标准的语法 */
}

.line2::after {
  content: "";
  width: 20%;
  height: 2rpx;
  position: absolute;
  z-index: 999;
  margin-top: 20rpx;
  left: 20%;
  background: #9b9b9b;
}
.line2::before{
  content: "";
  width: 20%;
  height: 2rpx;
  position: absolute;
  z-index: 999;
  margin-top: 20rpx;
  right: 20%;
    background: #9b9b9b;
}
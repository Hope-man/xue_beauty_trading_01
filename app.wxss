/* 渐变 自上而下  */
.W_jian_tb {
  background: -webkit-linear-gradient(#0086B3, #00BBDA); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(#0086B3, #00BBDA); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(#0086B3, #00BBDA); /* Firefox 3.6 - 15 */
  background: linear-gradient(#0086B3, #00BBDA); /* 标准的语法 */
}
/* 自左向右   */
.W_jian_lr {
  background: -webkit-linear-gradient(left, #FDA18B , #FD7081); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #FDA18B, #FD7081); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #FDA18B, #FD7081); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #FDA18B , #FD7081); /* 标准的语法 */
}
